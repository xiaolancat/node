<% include ./header %>

<!-- 路径导航 -->
<ol class="breadcrumb">
    <li>
        <a href="#">栏目管理</a>
    </li>
    <li class="active">编辑栏目</li>
</ol>
<!-- 路径导航 END -->
<!-- 表单 -->
<div class="container">
    <div class="row">
        <div class="col-md-4"></div>
        <div class="col-md-4">

            <form action="/admin/ahrefUpdate" class="userForm" method="POST">
                <input type="hidden" value="<%=data._id%>" name="_id">

                <div class="input-group">
                    <span class="input-group-addon">友链排序</span>
                    <input type="text" class="form-control" name="order" value="<%=data.order%>">
                </div>
                <div class="input-group">
                    <span class="input-group-addon">友链名称</span>
                    <input type="text" class="form-control" name="name" value="<%=data.name%>">
                </div>
                <div class="input-group">
                    <span class="input-group-addon">友链地址</span>
                    <input type="text" class="form-control" name="url" value="<%=data.url%>">
                </div>
                <div class="input-group">
                    <input type="submit">
                </div>
            </form>


        </div>
        <div class="col-md-4"></div>
    </div>

</div>
<!-- 表单 end -->








<% include ./footer %>