<% include ../admin/header %>
 <!-- 路径导航 -->
 <ol class="breadcrumb">
    <li>
        <a href="/test">管理页面</a>
    </li>
    <li class="active">管理员列表</li>
</ol>
    <!-- 路径导航 END -->
    <table class="table table-hover">
        <tr>
            <td>管理员</td>
            <td>电话</td>
            <td>创建时间</td>
            <td>操作</td>
        </tr>
        <% datalist.forEach(function(val){ %>
            <tr>
                <td><%=val.username%></td>
                <td><%=val.tel%></td>
                <td><%=val.ctime.toLocaleString()%></td>
                <td>
                    <a href="/test/adminEdit/<%=val._id%>">编辑</a>
                    <a href="/test/adminRemove/<%=val._id%>" onclick="return confirm('您确定要删除吗？')">删除</a>
                </td>
            </tr>
        <% })%>
    </table>

<% include ../admin/footer %>